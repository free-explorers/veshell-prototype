    "version": "0.2.0",
    "configurations": [
        {
            "name": "shell",
            "cwd": "shell",
            "request": "launch",
            "type": "dart",
            "env": {
                "XDG_CONFIG_HOME": "${workspaceFolder}/shell/.config",
            },
            "deviceId": "veshell",
            "preLaunchTask": "build_runner watch",
            "postDebugTask": "Terminate All Tasks"
        },
    ]
}